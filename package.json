  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "react-start": "react-app-rewired start",
    "react-build": "react-app-rewired build",
    "react-test": "react-app-rewired test --env=jsdom",
    "react-eject": "react-app-rewired eject",
    "electron-build": "electron-builder",
    "release": "yarn react-build && electron-builder --publish=always",
    "build": "yarn react-build && yarn electron-build",
    "start": "concurrently \"yarn dev:renderer\" \"wait-on http://localhost:3333 && yarn dev:electron\"",
    "dev:renderer": "cross-env BROWSER=none PORT=3333 yarn react-start",
    "dev:electron": "yarn build:electron:babel && electron .",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "build:electron:webpack": "cross-env NODE_ENV=development webpack --config webpack/electron.config.js --mode development",
    "build:electron:babel": "rm -rf dist && babel src/browser --out-dir dist/browser --copy-files --extensions '.ts,.js' && babel src/shared --out-dir dist/shared --copy-files --extensions '.ts,.js'"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
